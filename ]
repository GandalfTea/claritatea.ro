function digestInput(input) {
	const encoder = new TextEncoder();
	const data = encoder.encode(input);
	const hash = crypto.subtle.digest('SHA-512', data);
	return hash;
}

function checkKey(buttonPress) {
	const input = buttonPress.previousElementSibling.value;
	const hash = digestInput(input);
	console.log(input + "\n" + hash);
}
